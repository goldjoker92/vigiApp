    "android:bare": "cd android && ./gradlew installDebug",
    "fix-deps": "npx json -I -f package.json -e 'this.dependencies[\\\"react-native\\\"]=\\\"0.79.5\\\"; this.dependencies[\\\"react-native-web\\\"]=\\\"~0.19.10\\\"; this.dependencies[\\\"expo-notifications\\\"]=\\\"~0.31.4\\\"; this.dependencies[\\\"expo-av\\\"]=\\\"~15.1.7\\\"; this.dependencies[\\\"expo-clipboard\\\"]=\\\"~7.1.5\\\"; this.dependencies[\\\"expo-haptics\\\"]=\\\"~14.1.4\\\"; delete this.expo?.autolinking;'",
    "reset-env": "rimraf node_modules android/build .expo && npm ci && npx expo prebuild --clean",
    "bootstrap": "npm ci",
    "lock:lint": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm --empty-hostname false",
    "android:safe:debug": "pwsh -File ./tools/prebuild-android-safe.ps1 -Root . -Variant Debug",
    "android:safe:release": "pwsh -File ./tools/prebuild-android-safe.ps1 -Root . -Variant Release",
    "android:safe:noprebuild": "pwsh -File ./tools/prebuild-android-safe.ps1 -Root . -Variant Debug -NoPrebuild",
    "android:safe:fast": "pwsh -File ./tools/prebuild-android-safe.ps1 -Root . -Variant Debug -SkipExpoFix -NoPrebuild"
