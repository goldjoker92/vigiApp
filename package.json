{
  "name": "vigiapp",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "reset-project": "node ./scripts/reset-project.js",
    "prepare": "husky",
    "preinstall": "node -e \"const fs=require('fs'),c=require('crypto');const p='package-lock.json';const h=fs.existsSync(p)?c.createHash('sha256').update(fs.readFileSync(p)).digest('hex'):'MISSING';console.log('LOCK SHA256:',h,' SIZE:',fs.existsSync(p)?fs.statSync(p).size:0);\"",
    "eas-build-pre-install": "bash ./eas-build-pre-install.sh",
    "android:assemble": "cd android && ./gradlew assembleDebug --stacktrace --info",
    "android:install": "cd android && ./gradlew installDebug -x lint -x test",
    "android:clean-hard": "cd android && ./gradlew --stop || true && rm -rf ~/.gradle/caches ~/.gradle/wrapper android/.gradle android/app/build android/build && ./gradlew clean",
    "android:deps": "cd android && ./gradlew :app:dependencies > ../android-deps.txt",
    "android:bare": "cd android && ./gradlew installDebug",
    "android:bare:assemble": "cd android && ./gradlew :app:assemblePlayDebug",
    "android:bare:install": "cd android && ./gradlew :app:installPlayDebug",
    "test": "jest",
    "test:quick": "jest --watchAll=false --silent --passWithNoTests",
    "test:related": "jest --bail --findRelatedTests",
    "husky:report": "node scripts/husky-report-single.js",
    "deps:check": "expo install --check",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=18 <21"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,css,scss}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.jsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-.*|@react-navigation|expo(nent)?|@expo|expo-modules-core|@expo/vector-icons|@unimodules|unimodules-.*|sentry-expo)/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/",
      "/functions/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 100,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-firebase/app": "23.0.0",
    "@react-native-firebase/firestore": "23.0.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@stripe/stripe-react-native": "0.45.0",
    "dayjs": "^1.11.13",
    "expo": "53.0.22",
    "expo-av": "15.1.7",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "7.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "firebase": "12.0.0",
    "lucide-react-native": "^0.525.0",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "2.24.0",
    "react-native-google-mobile-ads": "^15.7.0",
    "react-native-maps": "1.20.1",
    "react-native-mask-input": "^1.2.3",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-paper": "^5.14.5",
    "react-native-purchases": "^9.4.2",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-svg": "15.11.2",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.20.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/helper-define-polyfill-provider": "^0.6.5",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@react-native-community/cli": "20.0.2",
    "@react-native-community/cli-platform-android": "20.0.2",
    "@react-native/gradle-plugin": "0.79.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^3.7.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "~19.0.10",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "expo-build-properties": "~0.14.8",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "react-dom": "19.0.0",
    "react-test-renderer": "^19.1.1",
    "scheduler": "^0.25.0",
    "schema-utils": "^4.3.2",
    "typescript": "~5.8.3",
    "webpack": "^5.101.3"
  },
  "overrides": {
    "react-dom": "19.0.0",
    "schema-utils": "^4.3.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "json-schema-traverse": "^1.0.0",
    "eslint": {
      "ajv": "6.12.6",
      "json-schema-traverse": "0.4.1"
    },
    "@eslint/eslintrc": {
      "ajv": "6.12.6",
      "json-schema-traverse": "0.4.1"
    }
  }
}
