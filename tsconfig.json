{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/index.jsx",
    "app/chats/[chatId].jsx"
  ],
  "extends": "expo/tsconfig.base"
}
