{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "node": "20.19.5",
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "cache": { "disabled": true },
      "env": { "EXPO_NODE_VERSION": "20.19.5" }
    },
    "preview": {
      "node": "20.19.5",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assemblePlayDebug"
      },
      "cache": { "disabled": true },
      "env": { "EXPO_NODE_VERSION": "20.19.5" }
    },
    "production": {
      "node": "20.19.5",
      "android": {
        "gradleCommand": ":app:bundlePlayRelease"
      },
      "cache": { "disabled": true },
      "env": { "EXPO_NODE_VERSION": "20.19.5" }
    }
  },
  "submit": {
    "production": {
      "android": { "track": "internal" }
    }
  }
}
