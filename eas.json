{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "appVersionSource": "remote",

    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "node": "20.11.1",
      "npm": "10.7.0",
      "android": { "buildType": "apk" },
      "cache": { "gradle": true, "npm": true, "yarn": true }
    },

    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "node": "20.11.1",
      "npm": "10.7.0",
      "android": { "buildType": "apk" },
      "cache": { "gradle": true, "npm": true, "yarn": true }
    },

    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "node": "20.11.1",
      "npm": "10.7.0",
      "android": { "buildType": "app-bundle" },
      "cache": { "gradle": true, "npm": true, "yarn": true }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
